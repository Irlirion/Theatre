<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="243.0" prefWidth="296.0" stylesheets="@style.css"
            xmlns="http://javafx.com/javafx/8" fx:controller="sample.view.LoginController">
    <children>
        <HBox layoutX="65.0" layoutY="159.0" prefHeight="32.0" prefWidth="166.0" spacing="20.0">
            <children>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOK" prefHeight="32.0"
                        prefWidth="80.0" text="Войти"/>
                <Button cancelButton="true" mnemonicParsing="false" onAction="#handleClose" prefHeight="32.0"
                        prefWidth="76.0" text="Отмена"/>
            </children>
        </HBox>
        <GridPane layoutX="48.0" layoutY="60.0" prefHeight="73.0" prefWidth="200.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Логин"/>
                <Label text="Пароль" GridPane.rowIndex="1"/>
                <TextField fx:id="loginField" GridPane.columnIndex="1"/>
                <PasswordField fx:id="passField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
